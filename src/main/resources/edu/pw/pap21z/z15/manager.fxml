<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane fx:controller="edu.pw.pap21z.z15.ManagerController" maxHeight="1.7976931348623157E308"
            maxWidth="1.7976931348623157E308" prefHeight="480.0"
            xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <HBox spacing="10.0" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <Region maxHeight="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
            <Label maxHeight="1.7976931348623157E308" text="Logged in as Manager 1"/>
            <Button maxHeight="1.7976931348623157E308" mnemonicParsing="false" text="Log out"/>
        </HBox>
    </top>
    <center>
        <VBox spacing="10.0" BorderPane.alignment="CENTER">
            <Label text="Warehouse contents"/>
            <TreeView fx:id="contentsTree" VBox.vgrow="ALWAYS"/>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </center>
    <right>
        <VBox spacing="10.0" BorderPane.alignment="CENTER">
            <Label text="Job queue"/>
            <ListView fx:id="queueList" VBox.vgrow="ALWAYS"/>
            <Label text="Workers"/>
            <ListView fx:id="workersList" VBox.vgrow="ALWAYS"/>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </right>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</BorderPane>
